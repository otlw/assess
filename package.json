{
  "name": "fathom-app",
  "version": "0.0.1",
  "description": "a decentralized learning network",
  "repository": "https://gitlab.com/fathom/assess",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "lint-js": "standard --verbose './app/**./*.ts' './app/**/*.js'| snazzy ",
    "fix-js": "standard --fix './app/index.js' './app/**/*.js' ",
    "lint-ts": "tslint --fix -c tslint.json ./app/**/*.ts -t verbose ; tslint -c tslint.json ./app/**/**/*.ts -t verbose",
    "lint-and-fix": "npm run fix-js && npm run lint-ts",
    "build": "webpack",
    "dev": "webpack-dev-server --open",
    "deploy-testnet": "cd node_modules/fathom-contracts && truffle migrate --reset",
    "full-deploy-testnet": "npm run deploy-testnet && node scripts/create2concepts2assessments.js && node scripts/completeAssessment.js -s stake && node scripts/completeAssessment.js -s commit && node scripts/completeAssessment.js -s reveal && node scripts/completeAssessment.js"
  },
  "author": "Jared Pereira",
  "license": "MIT",
  "standard": {
    "parser": "typescript-eslint-parser",
    "plugins": [
      "typescript"
    ],
    "globals": [
      "it",
      "contract",
      "assert",
      "describe",
      "artifacts",
      "web3"
    ]
  },
  "devDependencies": {
    "@types/lodash.throttle": "^4.1.4",
    "@types/node": "^10.10.1",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.7",
    "@types/react-hyperscript": "^3.0.3",
    "@types/react-redux": "^6.0.9",
    "@types/react-router-dom": "^4.3.1",
    "@types/redux-form": "^7.4.8",
    "@types/xtend": "^4.0.2",
    "enzyme": "^2.8.2",
    "eslint-plugin-typescript": "^0.12.0",
    "ethjs-abi": "^0.2.0",
    "fathom-contracts": "^1.2.2",
    "file-loader": "^1.1.11",
    "ganache-cli": "^6.1.0",
    "snazzy": "^7.1.1",
    "standard": "^11.0.1",
    "styled-components": "^3.2.5",
    "truffle": "4.1.7",
    "ts-loader": "^5.1.0",
    "tslint": "^5.11.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.1.3",
    "typescript-eslint-parser": "^19.0.2",
    "webcrypto-shim": "^0.1.4",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.12",
    "webpack-dev-server": "^3.1.1",
    "websocket": "^1.0.26"
  },
  "dependencies": {
    "bignumber.js": "^6.0.0",
    "buffer": "^5.1.0",
    "eth-dagger": "0.0.1",
    "ethereumjs-abi": "^0.6.4",
    "ipfs-api": "^22.2.4",
    "react": "^15.5.0",
    "react-dom": "^15.5.0",
    "react-hyperscript": "^3.2.0",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.2.2",
    "react-toggle-button": "^2.2.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "truffle-hdwallet-provider": "0.0.3",
    "web3": "^1.0.0-beta.33",
    "xtend": "^4.0.1"
  }
}
